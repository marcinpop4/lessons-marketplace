{
  "name": "lessons-marketplace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config frontend/vite.config.ts",
    "dev:server": "tsx watch server/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc --project frontend/tsconfig.app.json --skipLibCheck && vite build --config frontend/vite.config.ts",
    "build:shared": "tsc --project shared/tsconfig.shared.json --skipLibCheck",
    "build:server": "pnpm run build:shared && tsc --project server/tsconfig.server.json --skipLibCheck",
    "build:full": "npm run build && npm run build:server",
    "start": "node server/dist/index.js",
    "lint": "eslint .",
    "preview": "vite preview --config frontend/vite.config.ts",
    "prisma:generate": "prisma generate --schema=server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=server/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=server/prisma/schema.prisma",
    "prisma:seed": "tsx server/prisma/seed.ts",
    "db:reset": "prisma migrate reset --force --schema=server/prisma/schema.prisma"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.30.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@prisma/client": "^6.5.0",
    "@types/amqplib": "^0.10.4",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.24",
    "@types/pg": "^8.11.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "date-fns": "^4.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prisma": "^6.5.0",
    "tsx": "^4.7.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "cursor-tools": "latest"
  }
}
