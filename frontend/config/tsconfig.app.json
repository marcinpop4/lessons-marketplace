{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "baseUrl": "../..",
    "paths": {
      "@frontend": [
        "./frontend"
      ],
      "@frontend/*": [
        "./frontend/*"
      ],
      "@frontend/api/*": [
        "./frontend/api/*"
      ],
      "@frontend/components/*": [
        "./frontend/components/*"
      ],
      "@frontend/contexts/*": [
        "./frontend/contexts/*"
      ],
      "@frontend/hooks/*": [
        "./frontend/hooks/*"
      ],
      "@frontend/pages/*": [
        "./frontend/pages/*"
      ],
      "@frontend/styles/*": [
        "./frontend/styles/*"
      ],
      "@frontend/theme/*": [
        "./frontend/theme/*"
      ],
      "@frontend/types/*": [
        "./frontend/types/*"
      ],
      "@frontend/utils/*": [
        "./frontend/utils/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@/*": [
        "./frontend/*"
      ],
      "@config/*": [
        "./frontend/config/*"
      ]
    },
    "tsBuildInfoFile": "../../node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "useDefineForClassFields": true,
    "lib": [
      "es2020",
      "dom",
      "dom.iterable"
    ],
    "outDir": "../../dist/frontend",
    "rootDir": "../..",
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "emitDeclarationOnly": true,
    "jsx": "react-jsx",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "allowSyntheticDefaultImports": true,
    "resolvePackageJsonExports": true,
    "resolvePackageJsonImports": true,
    "preserveConstEnums": true,
    "incremental": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "strictBuiltinIteratorReturn": true,
    "alwaysStrict": true,
    "useUnknownInCatchVariables": true,
    "noEmitOnError": true,
    "sourceMap": true
  },
  "references": [
    {
      "path": "../../shared/tsconfig.shared.json"
    }
  ],
  "files": [
    "../theme.ts",
    "../api/apiClient.ts",
    "../api/buildApiUrl.ts",
    "../api/lessonApi.ts",
    "../api/lessonQuotesApi.ts",
    "../api/lessonRequestApi.ts",
    "../api/teacherQuoteApi.ts",
    "../components/features/lesson-confirmation/index.ts",
    "../components/shared/index.ts",
    "../components/shared/Alert/index.ts",
    "../components/shared/Badge/index.ts",
    "../components/shared/Button/index.ts",
    "../components/shared/Card/index.ts",
    "../components/theme-demo/index.ts",
    "../theme/index.ts",
    "../types/lesson.ts",
    "../types/theme.ts",
    "../utils/currencyFormatter.ts",
    "../utils/logger.ts",
    "../App.tsx",
    "../main.tsx",
    "../components/common/ProtectedRoute.tsx",
    "../components/features/lesson-confirmation/LessonDetails.tsx",
    "../components/features/lesson-request/AddressForm.tsx",
    "../components/features/lesson-request/LessonDetailsForm.tsx",
    "../components/features/lesson-request/LessonRequestForm.tsx",
    "../components/features/login/LoginForm.tsx",
    "../components/features/register/RegisterForm.tsx",
    "../components/features/teacher-dashboard/LessonRateForm.tsx",
    "../components/features/teacher-dashboard/LessonRateList.tsx",
    "../components/features/teacher-dashboard/TeacherLessonRatesManager.tsx",
    "../components/features/teacher-dashboard/TeacherProfile.tsx",
    "../components/features/teacher-dashboard/TeacherStats.tsx",
    "../components/features/teacher-quotes/LessonRequestCard.tsx",
    "../components/features/teacher-quotes/TeacherQuoteCard.tsx",
    "../components/features/teacher-quotes/TeacherQuotesList.tsx",
    "../components/shared/Alert/Alert.tsx",
    "../components/shared/Badge/Badge.tsx",
    "../components/shared/Button/Button.tsx",
    "../components/shared/Card/Card.tsx",
    "../components/shared/ColorSwatch/index.tsx",
    "../components/shared/FormattedAddress/index.tsx",
    "../components/shared/FormattedDateTime/index.tsx",
    "../components/shared/ThemeSwitcher/index.tsx",
    "../components/shared/ThemedCard/index.tsx",
    "../components/theme-demo/AlertsShowcase.tsx",
    "../components/theme-demo/BadgesShowcase.tsx",
    "../components/theme-demo/ButtonsShowcase.tsx",
    "../components/theme-demo/CardsShowcase.tsx",
    "../components/theme-demo/ColorPalettes.tsx",
    "../components/theme-demo/FormElements.tsx",
    "../components/theme-demo/HeaderElementsBackground.tsx",
    "../components/theme-demo/HeaderElementsCard.tsx",
    "../contexts/AuthContext.tsx",
    "../contexts/ThemeContext.tsx",
    "../pages/TeacherDashboard.tsx",
    "../pages/lesson-confirmation/index.tsx",
    "../pages/lesson-request/index.tsx",
    "../pages/login/index.tsx",
    "../pages/register/index.tsx",
    "../pages/teacher-dashboard/index.tsx",
    "../pages/teacher-quotes/index.tsx",
    "../pages/theme-demo/index.tsx"
  ],
  "include": [
    "../**/*.ts",
    "../**/*.tsx"
  ],
  "exclude": [
    "../node_modules"
  ]
}