POSTGRES_PASSWORD=postgres
POSTGRES_DB=lessons_marketplace
POSTGRES_HOST_AUTH_METHOD=scram-sha-256
DB_HOST=database
DB_PORT=5432
DB_SSL=false
DB_POOL_SIZE=10
